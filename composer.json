{
    "authors": [
        {
            "email": "dn@phantasie-schmiede.de",
            "name": "Daniel Ablass",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "PSBits\\ViewHelpers\\": "Classes/"
        }
    },
    "config": {
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "vendor-dir": ".Build/vendor"
    },
    "description": "Collection of useful ViewHelpers",
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "extension-key": "view_helpers",
            "web-dir": ".Build/public"
        }
    },
    "keywords": [
        "configuration",
        "development",
        "extension",
        "typo3"
    ],
    "license": "GPL-3.0-or-later",
    "name": "psbits/view-helpers",
    "require": {
        "php": "^8.3",
        "psbits/foundation": "^3.0",
        "typo3/cms-core": "^12.4 || ^13.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "scripts": {
        "prepare-release": [
            "rm -rf .github",
            "rm .editorconfig",
            "rm .gitattributes",
            "rm .gitignore"
        ]
    },
    "type": "typo3-cms-extension"
}
