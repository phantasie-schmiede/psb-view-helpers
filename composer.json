{
	"authors": [
		{
			"email": "dn@phantasie-schmiede.de",
			"name": "Daniel Ablass",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"PSB\\PsbViewHelpers\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PSB\\PsbViewHelpers\\Tests\\": "Tests/"
		}
	},
	"config": {
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"description": "Collection of useful ViewHelpers",
	"extra": {
		"typo3/cms": {
			"app-dir": ".Build",
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"extension-key": "psb_view_helpers",
			"web-dir": ".Build/public"
		}
	},
	"keywords": [
		"configuration",
		"development",
		"extension",
		"typo3"
	],
	"license": "GPL-3.0-or-later",
	"name": "psb/psb-view-helpers",
	"require": {
		"php": "^8.1",
		"psb/psb-foundation": "^2.0",
		"typo3/cms-core": "^11.5.5"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"typo3/testing-framework": "^6.8"
	},
	"scripts": {
		"prepare-release": [
			"rm -rf .github",
			"rm .editorconfig",
			"rm .gitattributes",
			"rm .gitignore"
		]
	},
	"type": "typo3-cms-extension"
}
